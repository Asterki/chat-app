{
    "name": "dimlim",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "nodemon --config ./nodemon.json ./server/index.ts --dev true",
        "start": "npm-run-all start:*",
        "start:clean": "rm -rf ./build/*",
        "start:build-server": "tsc -p ./server/tsconfig.json",
        "start:build-client": "next build",
        "start:move-static": "cp -r ./locales ./build/ && cp ./src/assets -r ./build/src && cp -r ./public ./build/",
        "start:run-server": "node ./build/server/index.js --dev false",
        "build": "next build",
        "lint": "next lint",
        "setup": "mkdir data && cd data && mkdir avatars && mkdir files && cd .. && npm i -y && mkdir ./server/logs/ && touch ./server/logs/errors.log && ts-node ./server/index.ts --setup"
    },
